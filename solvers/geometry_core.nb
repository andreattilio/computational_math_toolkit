               RowBox[{"i", ",", "1", ",", 
                RowBox[{"Length", "[", "vertices", "]"}]}], "}"}]}], "]"}]}], 
           "}"}]}], "}"}], ",", 
        RowBox[{"Frame", "->", "True"}], ",", 
        RowBox[{"Axes", "->", "True"}], ",", 
        RowBox[{"AxesLabel", "->", 
         RowBox[{"{", 
          RowBox[{"\"\<x\>\"", ",", "\"\<y\>\""}], "}"}]}], ",", 
