               RowBox[{"0", ",", "1"}], "}"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", 
              RowBox[{"Length", "[", "vertices", "]"}]}], "}"}]}], "]"}], ",", 
          RowBox[{"(*", 
