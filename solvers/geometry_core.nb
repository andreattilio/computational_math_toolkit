           RowBox[{"Original", " ", "polygon"}], "*)"}], 
          RowBox[{"{", 
           RowBox[{"LightOrange", ",", 
            RowBox[{"Opacity", "[", "1", "]"}], ",", "originalPolygon"}], 
           "}"}], ",", 
          RowBox[{"(*", 
           RowBox[{"Center", " ", "point"}], "*)"}], 
          RowBox[{"{", 
           RowBox[{"Red", ",", 
            RowBox[{"PointSize", "[", "0.02", "]"}], ",", 
            RowBox[{"Point", "[", "center", "]"}]}], "}"}], ",", 
          RowBox[{"Text", "[", 
