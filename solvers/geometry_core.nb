      "\[IndentingNewLine]", 
      RowBox[{"yMax", "=", 
       RowBox[{
        RowBox[{"Max", "[", 
         RowBox[{"allPoints", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "2"}], "]"}], "]"}], "]"}], "+", "1"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
