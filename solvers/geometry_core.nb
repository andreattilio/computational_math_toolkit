Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"enlargePolygon", "[", 
    RowBox[{"vertices_", ",", "scaleFactor_", ",", "center_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"originalPolygon", ",", "enlargedPolygon", ",", "plotTitle"}], 
      "}"}], ",", 
     RowBox[{"(*", 
      RowBox[{
      "Create", " ", "the", " ", "original", " ", "polygon", " ", "from", " ",
        "vertices"}], "*)"}], 
     RowBox[{
      RowBox[{"originalPolygon", "=", 
       RowBox[{"ConvexHullMesh", "[", "vertices", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Create", " ", "the", " ", "enlarged", " ", "polygon", " ", "using", " ",
         "geometric", " ", "transformation"}], "*)"}], 
      RowBox[{"enlargedPolygon", "=", 
       RowBox[{"GeometricTransformation", "[", 
        RowBox[{"originalPolygon", ",", 
         RowBox[{"ScalingTransform", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"scaleFactor", ",", "scaleFactor"}], "}"}], ",", 
           "center"}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Create", " ", "plot", " ", "title"}], "*)"}], 
      RowBox[{"plotTitle", "=", 
       RowBox[{"\"\<Polygon Enlargement with Scale Factor = \>\"", "<>", 
        RowBox[{"ToString", "[", "scaleFactor", "]"}], 
        "<>", "\"\< from Center \>\"", "<>", 
        RowBox[{"ToString", "[", "center", "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Determine", " ", "an", " ", "appropriate", " ", "plot", " ", "range", 
        " ", "based", " ", "on", " ", "original", " ", "and", " ", "enlarged",
         " ", "vertices"}], "*)"}], 
      RowBox[{"allPoints", "=", 
       RowBox[{"Join", "[", 
        RowBox[{"vertices", ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"center", "+", 
            RowBox[{"scaleFactor", "*", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"vertices", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "-", "center"}], ")"}]}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", 
             RowBox[{"Length", "[", "vertices", "]"}]}], "}"}]}], "]"}], ",", 
         
         RowBox[{"{", "center", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"xMin", "=", 
       RowBox[{
        RowBox[{"Min", "[", 
         RowBox[{"allPoints", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}], "-", "1"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"xMax", "=", 
       RowBox[{
        RowBox[{"Max", "[", 
         RowBox[{"allPoints", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}], "+", "1"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"yMin", "=", 
